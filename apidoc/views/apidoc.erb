<% root_url = 'apidoc' %>

<!DOCTYPE html>
<html>
<head>
  <title>IoSL @ TU Berlin (WiSe 15/16) Backend API</title>
  <meta charset="UTF-8">
  <link href='<%= root_url %>/css/typography.css' media='screen' rel='stylesheet' type='text/css'/>
  <link href='<%= root_url %>/css/reset.css' media='screen' rel='stylesheet' type='text/css'/>
  <link href='<%= root_url %>/css/screen.css' media='screen' rel='stylesheet' type='text/css'/>
  <link href='<%= root_url %>/css/reset.css' media='print' rel='stylesheet' type='text/css'/>
  <link href='<%= root_url %>/css/screen.css' media='print' rel='stylesheet' type='text/css'/>
  <script type="text/javascript" src="<%= root_url %>/js/lib/shred.bundle.js"></script>
  <script src='<%= root_url %>/js/lib/jquery-1.8.0.min.js' type='text/javascript'></script>
  <script src='<%= root_url %>/js/lib/jquery.slideto.min.js' type='text/javascript'></script>
  <script src='<%= root_url %>/js/lib/jquery.wiggle.min.js' type='text/javascript'></script>
  <script src='<%= root_url %>/js/lib/jquery.ba-bbq.min.js' type='text/javascript'></script>
  <script src='<%= root_url %>/js/lib/handlebars-2.0.0.js' type='text/javascript'></script>
  <script src='<%= root_url %>/js/lib/underscore-min.js' type='text/javascript'></script>
  <script src='<%= root_url %>/js/lib/backbone-min.js' type='text/javascript'></script>
  <script src='<%= root_url %>/js/lib/swagger-client.js' type='text/javascript'></script>
  <script src='<%= root_url %>/js/swagger-ui.js' type='text/javascript'></script>
  <script src='<%= root_url %>/js/lib/highlight.7.3.pack.js' type='text/javascript'></script>
  <script src='<%= root_url %>/js/lib/marked.js' type='text/javascript'></script>

  <script type="text/javascript">
    $(function () {
      window.swaggerUi = new SwaggerUi({
        url: "<%= api_url %>",
        dom_id: "swagger-ui-container",
        supportedSubmitMethods: ['get', 'post', 'put', 'delete', 'patch'],
        onComplete: function(swaggerApi, swaggerUi){
          $('pre code').each(function(i, e) {
            hljs.highlightBlock(e)
          });
        },
        onFailure: function(data) {
          log("Unable to Load SwaggerUI");
        },
        docExpansion: "none",
        sorter : "alpha"
      });

      window.swaggerUi.load();
  });
  </script>
</head>

<body class="swagger-section">
<div id='header'>
  <div class="swagger-ui-wrap">
    <h1>IoSL @ TU Berlin (WiSe 15/16) Backend API</h1>
  </div>
</div>

<div id="message-bar" class="swagger-ui-wrap">&nbsp;</div>
<div id="swagger-ui-container" class="swagger-ui-wrap"></div>
</body>
</html>
